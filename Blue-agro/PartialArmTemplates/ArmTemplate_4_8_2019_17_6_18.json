{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "Blue-agro"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/pipeline1')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy Data1",
						"type": "Copy",
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"typeProperties": {
							"source": {
								"type": "BlobSource",
								"recursive": true
							},
							"sink": {
								"type": "AzureTableSink",
								"azureTableInsertType": "merge",
								"azureTablePartitionKeyName": {
									"value": "@pipeline().RunId",
									"type": "Expression"
								},
								"writeBatchSize": 10000
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"columnMappings": {
									"PartitionKey": "PartitionKey",
									"RowKey": "RowKey",
									"Timestamp": "Timestamp",
									"clientname": "clientname",
									"Prop_0": "Prop_0",
									"Prop_1": "Prop_1",
									"Prop_2": "Prop_2",
									"Prop_3": "Prop_3",
									"Prop_4": "Prop_4",
									"Prop_5": "Prop_5",
									"Prop_6": "Prop_6",
									"Prop_7": "Prop_7",
									"Prop_8": "Prop_8",
									"Prop_9": "Prop_9",
									"Prop_10": "Prop_10",
									"Prop_11": "Prop_11",
									"Prop_12": "Prop_12",
									"Prop_13": "Prop_13"
								}
							}
						},
						"inputs": [
							{
								"referenceName": "entradatable",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
								"referenceName": "bluetabela",
								"type": "DatasetReference"
							}
						]
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/entradatable')]",
				"[concat(variables('factoryId'), '/datasets/bluetabela')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/Azure_saida2')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "conecntbaws",
					"type": "LinkedServiceReference"
				},
				"type": "AzureBlob",
				"structure": [
					{
						"name": "Pop_0"
					},
					{
						"name": "Pop_1"
					},
					{
						"name": "Pop_2"
					},
					{
						"name": "Pop_3"
					},
					{
						"name": "Pop_4"
					},
					{
						"name": "Pop_5"
					},
					{
						"name": "Pop_6"
					},
					{
						"name": "Pop_7"
					},
					{
						"name": "Pop_8"
					},
					{
						"name": "Pop_9"
					},
					{
						"name": "Pop_10"
					},
					{
						"name": "Pop_11"
					},
					{
						"name": "Pop_12"
					},
					{
						"name": "Pop_13"
					},
					{
						"name": "clientname"
					},
					{
						"name": "date"
					}
				],
				"typeProperties": {
					"format": {
						"type": "TextFormat",
						"columnDelimiter": ",",
						"rowDelimiter": "",
						"nullValue": "\\N",
						"treatEmptyAsNull": true,
						"skipLineCount": 0,
						"firstRowAsHeader": false
					},
					"folderPath": {
						"value": "@concat('proj-agro/SAIDA/', utcnow('MMddyyyy'))",
						"type": "Expression"
					}
				}
			}
		},
		{
			"name": "[concat(parameters('factoryName'), '/azure_entrada')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "conectazurre",
					"type": "LinkedServiceReference"
				},
				"type": "AzureBlob",
				"typeProperties": {
					"format": {
						"type": "TextFormat",
						"columnDelimiter": ",",
						"rowDelimiter": "",
						"nullValue": "\\N",
						"treatEmptyAsNull": true,
						"skipLineCount": 0,
						"firstRowAsHeader": false
					},
					"fileName": "",
					"folderPath": {
						"value": "@concat('proj-agro/ENTRADA/','20190404/','met/','*.csv')",
						"type": "Expression"
					}
				}
			}
		},
		{
			"name": "[concat(parameters('factoryName'), '/azure_saida1')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "saidaconect1",
