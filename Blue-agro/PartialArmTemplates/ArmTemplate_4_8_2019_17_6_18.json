				}
			}
		},
		{
			"name": "[concat(parameters('factoryName'), '/entradatable')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureBlobStorage3",
					"type": "LinkedServiceReference"
				},
				"type": "AzureBlob",
				"structure": [
					{
