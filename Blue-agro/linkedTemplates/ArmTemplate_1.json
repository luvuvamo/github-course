{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "Blue-agro"
		},
		"entradatable_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "@concat('*.csv')"
		},
		"entradatable_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "proj-agro/ENTRADA/20190404/met"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/entradatable')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureBlobStorage3",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureBlob",
				"structure": [
					{
						"name": "PartitionKey"
					},
					{
						"name": "RowKey"
					},
					{
						"name": "Timestamp",
						"type": "DateTime"
					},
					{
						"name": "clientname"
					},
					{
						"name": "Prop_0"
					},
					{
						"name": "Prop_1"
					},
					{
						"name": "Prop_2"
					},
					{
						"name": "Prop_3"
					},
					{
						"name": "Prop_4"
					},
					{
						"name": "Prop_5"
					},
					{
						"name": "Prop_6"
					},
					{
						"name": "Prop_7"
					},
					{
						"name": "Prop_8"
					},
					{
						"name": "Prop_9"
					},
					{
						"name": "Prop_10"
					},
					{
						"name": "Prop_11"
					},
					{
						"name": "Prop_12"
					},
					{
						"name": "Prop_13"
					}
				],
				"typeProperties": {
					"format": {
						"type": "TextFormat",
						"columnDelimiter": ",",
						"rowDelimiter": "",
						"nullValue": "\\N",
						"treatEmptyAsNull": true,
						"skipLineCount": 0,
						"firstRowAsHeader": false
					},
					"fileName": {
						"value": "[parameters('entradatable_properties_typeProperties_fileName')]",
						"type": "Expression"
					},
					"folderPath": "[parameters('entradatable_properties_typeProperties_folderPath')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/aws-agro')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "aws_entrada",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "FileSystemSource",
								"recursive": true
							},
							"sink": {
								"type": "BlobSink"
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"columnMappings": {
									"Prop_0": "Pop_0",
									"Prop_1": "Pop_1",
									"Prop_2": "Pop_2",
									"Prop_3": "Pop_3",
									"Prop_4": "Pop_4",
									"Prop_5": "Pop_5",
									"Prop_6": "Pop_6",
									"Prop_7": "Pop_7",
									"Prop_8": "Pop_8",
									"Prop_9": "Pop_9",
									"Prop_10": "Pop_10",
									"Prop_11": "Pop_11",
									"Prop_12": "Pop_12",
									"Prop_13": "Pop_13",
									"clientname": "clientname",
									"date": "date"
								}
							}
						},
						"inputs": [
							{
								"referenceName": "aws_entrada",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
								"referenceName": "Azure_saida2",
								"type": "DatasetReference",
								"parameters": {}
							}
						]
					}
				],
				"annotations": []
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/blob-agro')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "azure-agro",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "BlobSource",
								"recursive": true
							},
							"sink": {
								"type": "BlobSink"
							},
							"enableStaging": false
						},
						"inputs": [
							{
								"referenceName": "azure_entrada",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
								"referenceName": "azure_saida1",
								"type": "DatasetReference",
								"parameters": {}
							}
						]
					}
				],
				"annotations": []
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/pipeline1')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy Data1",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [
							{
								"name": "Source",
								"value": "proj-agro/ENTRADA/20190404/met/@{concat('*.csv')}"
							},
							{
								"name": "Destination",
								"value": "teste01"
							}
						],
						"typeProperties": {
							"source": {
								"type": "BlobSource",
								"recursive": true
							},
							"sink": {
								"type": "AzureTableSink",
								"azureTableInsertType": "merge",
								"azureTablePartitionKeyName": {
									"value": "@pipeline().RunId",
									"type": "Expression"
								},
								"writeBatchSize": 10000
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"columnMappings": {
									"PartitionKey": "PartitionKey",
									"RowKey": "RowKey",
									"Timestamp": "Timestamp",
									"clientname": "clientname",
									"Prop_0": "Prop_0",
									"Prop_1": "Prop_1",
									"Prop_2": "Prop_2",
									"Prop_3": "Prop_3",
									"Prop_4": "Prop_4",
									"Prop_5": "Prop_5",
									"Prop_6": "Prop_6",
									"Prop_7": "Prop_7",
									"Prop_8": "Prop_8",
									"Prop_9": "Prop_9",
									"Prop_10": "Prop_10",
									"Prop_11": "Prop_11",
									"Prop_12": "Prop_12",
									"Prop_13": "Prop_13"
								}
							}
						},
						"inputs": [
							{
								"referenceName": "entradatable",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
								"referenceName": "bluetabela",
								"type": "DatasetReference",
								"parameters": {
									"clientname": "clientblue"
								}
							}
						]
					}
				],
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/entradatable')]"
			]
		}
	]
}