{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "Blue-agro"
		},
		"conect_entrada_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'conect_entrada'"
		},
		"conect_saida_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'conect_saida'"
		},
		"conectablesaida_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'conectablesaida'"
		},
		"conectdatasetexecutor_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'conectdatasetexecutor'"
		},
		"conectdatasetget_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'conectdatasetget'"
		},
		"datasetexecutor_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"datasetexecutor_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "@concat('proj-agro/ENTRADA/', utcnow('yyyyMMdd'), '/', dataset().sourcefolder)"
		},
		"datasetget_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"datasetget_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "@concat('proj-agro/ENTRADA/', utcnow('yyyyMMdd'), '/', dataset().sourcefolder)"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"conect_entrada_connectionString": {
						"value": "[parameters('conect_entrada_connectionString')]"
					},
					"conect_saida_connectionString": {
						"value": "[parameters('conect_saida_connectionString')]"
					},
					"conectablesaida_connectionString": {
						"value": "[parameters('conectablesaida_connectionString')]"
					},
					"conectdatasetexecutor_connectionString": {
						"value": "[parameters('conectdatasetexecutor_connectionString')]"
					},
					"conectdatasetget_connectionString": {
						"value": "[parameters('conectdatasetget_connectionString')]"
					},
					"datasetexecutor_properties_typeProperties_fileName": {
						"value": "[parameters('datasetexecutor_properties_typeProperties_fileName')]"
					},
					"datasetexecutor_properties_typeProperties_folderPath": {
						"value": "[parameters('datasetexecutor_properties_typeProperties_folderPath')]"
					},
					"datasetget_properties_typeProperties_fileName": {
						"value": "[parameters('datasetget_properties_typeProperties_fileName')]"
					},
					"datasetget_properties_typeProperties_folderPath": {
						"value": "[parameters('datasetget_properties_typeProperties_folderPath')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}