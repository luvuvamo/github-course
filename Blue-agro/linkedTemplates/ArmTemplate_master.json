{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "Blue-agro"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureBlobStorage2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage2'"
		},
		"aws_conect_entrada_secretAccessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'secretAccessKey' of 'aws_conect_entrada'"
		},
		"aws_entrada_secretAccessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'secretAccessKey' of 'aws_entrada'"
		},
		"conecntbaws_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'conecntbaws'"
		},
		"conect_entrada_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'conect_entrada'"
		},
		"conect_saida_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'conect_saida'"
		},
		"conectazurre_connectionString": {
			"type": "secureString",
