			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'conectentrada'"
		},
		"conectsaida_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'conectsaida'"
		},
		"saida002_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'saida002'"
		},
		"saidaconect1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'saidaconect1'"
		},
		"Azure_saida2_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "@utcnow('MMddyyyyhhmms')"
		},
		"Azure_saida2_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "proj-agro/SAIDA/20190404"
		},
		"azure_entrada_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"azure_entrada_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "proj-agro/ENTRADA/20190403"
		},
		"azure_saida1_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"azure_saida1_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "@concat('proj-agro/SAIDA', utcnow('MMddyyyy'))"
		},
		"aws_conect_entrada_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "AKIAX2RO257TXINSITXE"
		},
		"aws_entrada_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "AKIAX2RO257TXINSITXE"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureBlobStorage2_connectionString": {
						"value": "[parameters('AzureBlobStorage2_connectionString')]"
					},
					"aws_conect_entrada_secretAccessKey": {
						"value": "[parameters('aws_conect_entrada_secretAccessKey')]"
					},
					"aws_entrada_secretAccessKey": {
						"value": "[parameters('aws_entrada_secretAccessKey')]"
					},
					"conecntbaws_connectionString": {
						"value": "[parameters('conecntbaws_connectionString')]"
					},
					"conect_entrada_connectionString": {
						"value": "[parameters('conect_entrada_connectionString')]"
					},
					"conect_saida_connectionString": {
						"value": "[parameters('conect_saida_connectionString')]"
					},
					"conectazurre_connectionString": {
						"value": "[parameters('conectazurre_connectionString')]"
					},
					"conectentrada_connectionString": {
						"value": "[parameters('conectentrada_connectionString')]"
					},
					"conectsaida_connectionString": {
						"value": "[parameters('conectsaida_connectionString')]"
					},
					"saida002_connectionString": {
						"value": "[parameters('saida002_connectionString')]"
					},
					"saidaconect1_connectionString": {
						"value": "[parameters('saidaconect1_connectionString')]"
					},
					"aws_conect_entrada_properties_typeProperties_accessKeyId": {
						"value": "[parameters('aws_conect_entrada_properties_typeProperties_accessKeyId')]"
					},
					"aws_entrada_properties_typeProperties_accessKeyId": {
						"value": "[parameters('aws_entrada_properties_typeProperties_accessKeyId')]"
					},
					"Azure_saida2_properties_typeProperties_fileName": {
						"value": "[parameters('Azure_saida2_properties_typeProperties_fileName')]"
					},
					"Azure_saida2_properties_typeProperties_folderPath": {
						"value": "[parameters('Azure_saida2_properties_typeProperties_folderPath')]"
					},
					"azure_entrada_properties_typeProperties_fileName": {
						"value": "[parameters('azure_entrada_properties_typeProperties_fileName')]"
					},
					"azure_entrada_properties_typeProperties_folderPath": {
						"value": "[parameters('azure_entrada_properties_typeProperties_folderPath')]"
					},
					"azure_saida1_properties_typeProperties_fileName": {
						"value": "[parameters('azure_saida1_properties_typeProperties_fileName')]"
					},
					"azure_saida1_properties_typeProperties_folderPath": {
						"value": "[parameters('azure_saida1_properties_typeProperties_folderPath')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}