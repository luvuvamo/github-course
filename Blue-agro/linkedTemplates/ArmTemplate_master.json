{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "Blue-agro"
		},
		"AzureBlobStorage3_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage3'"
		},
		"bluetable_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'bluetable'"
		},
		"conecntbaws_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'conecntbaws'"
		},
		"conect_entrada_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'conect_entrada'"
		},
		"conect_saida_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'conect_saida'"
		},
		"conectazurre_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'conectazurre'"
		},
		"saidaconect1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'saidaconect1'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage3_connectionString": {
						"value": "[parameters('AzureBlobStorage3_connectionString')]"
					},
					"bluetable_connectionString": {
						"value": "[parameters('bluetable_connectionString')]"
					},
					"conecntbaws_connectionString": {
						"value": "[parameters('conecntbaws_connectionString')]"
					},
					"conect_entrada_connectionString": {
						"value": "[parameters('conect_entrada_connectionString')]"
					},
					"conect_saida_connectionString": {
						"value": "[parameters('conect_saida_connectionString')]"
					},
					"conectazurre_connectionString": {
						"value": "[parameters('conectazurre_connectionString')]"
					},
					"saidaconect1_connectionString": {
						"value": "[parameters('saidaconect1_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}