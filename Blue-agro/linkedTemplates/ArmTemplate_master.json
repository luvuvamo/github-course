{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "Blue-agro"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureBlobStorage2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage2'"
		},
		"conect_entrada_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'conect_entrada'"
		},
		"conect_saida_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'conect_saida'"
		},
		"conectentrada_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'conectentrada'"
		},
		"conectsaida_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'conectsaida'"
		},
		"entrada_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "client1.csv"
		},
		"entrada_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "proj-agro/ENTRADA/20190403"
		},
		"saida_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"saida_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "proj-agro/SAIDA/20190403"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureBlobStorage2_connectionString": {
						"value": "[parameters('AzureBlobStorage2_connectionString')]"
					},
					"conect_entrada_connectionString": {
						"value": "[parameters('conect_entrada_connectionString')]"
					},
					"conect_saida_connectionString": {
						"value": "[parameters('conect_saida_connectionString')]"
					},
					"conectentrada_connectionString": {
						"value": "[parameters('conectentrada_connectionString')]"
					},
					"conectsaida_connectionString": {
						"value": "[parameters('conectsaida_connectionString')]"
					},
					"entrada_properties_typeProperties_fileName": {
						"value": "[parameters('entrada_properties_typeProperties_fileName')]"
					},
					"entrada_properties_typeProperties_folderPath": {
						"value": "[parameters('entrada_properties_typeProperties_folderPath')]"
					},
					"saida_properties_typeProperties_fileName": {
						"value": "[parameters('saida_properties_typeProperties_fileName')]"
					},
					"saida_properties_typeProperties_folderPath": {
						"value": "[parameters('saida_properties_typeProperties_folderPath')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}