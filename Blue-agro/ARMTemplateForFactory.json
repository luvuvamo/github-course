{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "Blue-agro"
		},
		"conect_entrada_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'conect_entrada'"
		},
		"conect_saida_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'conect_saida'"
		},
		"conectablesaida_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'conectablesaida'"
		},
		"conectdatasetexecutor_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'conectdatasetexecutor'"
		},
		"conectdatasetget_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'conectdatasetget'"
		},
		"datasetexecutor_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"datasetexecutor_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "@concat('proj-agro/ENTRADA/', utcnow('yyyyMMdd'), '/', dataset().sourcefolder)"
		},
		"datasetget_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"datasetget_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "@concat('proj-agro/ENTRADA/', utcnow('yyyyMMdd'), '/', dataset().sourcefolder)"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/geoline')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Getgeol",
						"type": "GetMetadata",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"dataset": {
								"referenceName": "datasetget",
								"type": "DatasetReference",
								"parameters": {
									"sourcefolder": "geol"
								}
							},
							"fieldList": [
								"childItems"
							]
						}
					},
					{
						"name": "ForEach",
						"type": "ForEach",
						"dependsOn": [
							{
								"activity": "Getgeol",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"userProperties": [],
						"typeProperties": {
							"items": {
								"value": "@activity('Getgeol').output.childItems",
								"type": "Expression"
							},
							"activities": [
								{
									"name": "executorgeol",
									"type": "Copy",
									"dependsOn": [],
									"policy": {
										"timeout": "7.00:00:00",
										"retry": 0,
										"retryIntervalInSeconds": 30,
										"secureOutput": false,
										"secureInput": false
									},
									"userProperties": [],
									"typeProperties": {
										"source": {
											"type": "BlobSource",
											"recursive": true
										},
										"sink": {
											"type": "AzureTableSink",
											"azureTableInsertType": "merge",
											"azureTableDefaultPartitionKeyValue": {
												"value": "@item().name",
												"type": "Expression"
											},
											"writeBatchSize": 10003
										},
										"enableStaging": false
									},
									"inputs": [
										{
											"referenceName": "datasetexecutor",
											"type": "DatasetReference",
											"parameters": {
												"sourcefolder": "geol"
											}
										}
									],
									"outputs": [
										{
											"referenceName": "saidaTable",
											"type": "DatasetReference",
											"parameters": {
												"table": "geolocalizacao"
											}
										}
									]
								}
							]
						}
					}
				],
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/datasetget')]",
				"[concat(variables('factoryId'), '/datasets/datasetexecutor')]",
				"[concat(variables('factoryId'), '/datasets/saidaTable')]"
			]
		},
