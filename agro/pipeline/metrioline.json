{
	"name": "metrioline",
	"properties": {
		"activities": [
			{
				"name": "Getmet",
				"type": "GetMetadata",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"dataset": {
						"referenceName": "datasetget",
						"type": "DatasetReference",
						"parameters": {
							"sourcefolder": "met"
						}
					},
					"fieldList": [
						"childItems"
					]
				}
			},
			{
				"name": "ForEach1",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "Getmet",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"typeProperties": {
					"items": {
						"value": "@activity('Getmet').output.childItems",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "executormet",
							"type": "Copy",
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"typeProperties": {
								"source": {
									"type": "BlobSource",
									"recursive": true
								},
								"sink": {
									"type": "AzureTableSink",
									"azureTableInsertType": "merge",
									"azureTableDefaultPartitionKeyValue": {
										"value": "@item().name",
										"type": "Expression"
									},
									"writeBatchSize": 10003
								},
								"enableStaging": false
							},
							"inputs": [
								{
									"referenceName": "datasetexecutor",
									"type": "DatasetReference",
									"parameters": {
										"sourcefolder": "met"
									}
								}
							],
							"outputs": [
								{
									"referenceName": "saidaTable",
									"type": "DatasetReference",
									"parameters": {
										"table": "metereologia"
									}
								}
							]
						}
					]
				}
			}
		]
	}
}